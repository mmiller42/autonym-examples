    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "nodemon": "^1.12.1"
  },
  "dependencies": {
    "autonym": "^1.0.3",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.0.0",
    "express": "^4.0.0"
